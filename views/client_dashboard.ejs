            <h1 class="h3 mb-2 text-gray-800">Cases</h1>
            <p class="mb-4">Contains all the active and inactive cases</p>

            <!-- DataTales Example -->
            <div class="card shadow mb-4">
              <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Active Cases</h6>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table
                    class="table table-bordered"
                    id="dataTable-3"
                    width="100%"
                    cellspacing="0"
                  >
                    <thead>
                      <tr>
                        <th>Case Type</th>
                        <th>Case Name</th>
                        <th>Description</th>
                        <th>Case No</th>
                        <th>Hearing Date</th>
                        <th>Details</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% cases.forEach ( (case1) => { if(case1.isResolved ===
                      "N") { %>
                      <tr>
                        <td><%= case1.case_type %></td>
                        <td><%= case1.case_name %></td>
                        <td><%= case1.case_descp %></td>
                        <td><%= case1.court_case_no %></td>
                        <td><%= case1.h_date %></td>
                        <td>
                          <form
                            action="/client/display_case_details/<%= case1._id %>"
                            method="GET"
                          >
                            <button type="submit" class="btn btn-primary">
                              Further Details
                            </button>
                          </form>
                        </td>
                      </tr>
                      <% } } ); %>
                    </tbody>
                    <tfoot>
                      <tr>
                        <th>Case Type</th>
                        <th>Case Name</th>
                        <th>Description</th>
                        <th>Case No</th>
                        <th>Hearing Date</th>
                        <th>Details</th>
                      </tr>
                    </tfoot>
                  </table>
                </div>
              </div>
            </div>
            <div class="card shadow mb-4">
              <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">
                  Resolved Cases
                </h6>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table
